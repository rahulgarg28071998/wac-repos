   "outputs": [],
   "source": [
    "def entropy(p1,p2):\n",
    "    ans = 0\n",
    "    if p1!=0:\n",
    "        ans += -p1*np.log2(p1)\n",
    "    if p2!=0:\n",
    "        ans += -p2*np.log2(p2)\n",
    "    return ans"
   ]
